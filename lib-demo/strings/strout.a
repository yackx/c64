; Print a string using the strout routine.

        ;ACME 0.97
        !cpu 6510

        !src <lib/registers/pseudo-registers.a>
        
        * = $c000


main:
        lda #<.hello
        sta R1
        lda #>.hello
        sta R2
        jsr strout     
        rts

.hello  !pet "hello, world!", 13, 0

        !src <lib/strings/strout.a>
