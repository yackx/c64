; Print "hello 64" to the screen using the KERNAL routine at $ffd2

            !cpu 6510
            * = $c000

            !src <lib/mmap/kernal.a>


main:
            ldx #0
            beq +
-           jsr CHROUT
            inx
+           lda hello,x
            bne -
            rts

hello:      !pet "hello 64", 13, 0
