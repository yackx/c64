; Busy wait for approx R5 * R6 * 5 cycles
;
; IN:
; R5 Outer wait counter
; R6 Inner wait counter
;
; USE: -

        !src <common/registers.a>

busy_wait:
        +push_all

        ldy R5
--      dey
        beq +
        ldx R6
-       dex
        bne -
        beq --

+       +pull_all
        rts
