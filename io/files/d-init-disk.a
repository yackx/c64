; Demo - Send init disk command to drive 8
;
; You may first simulate an error by loading a non-existent file
; or by accessing a non-existent drive.

            ;ACME 0.97
            !cpu 6510

            * = $c000

main:
            ldx #8
            jsr init_disk
            rts


            !src <io/files/cmd-channel.a>

