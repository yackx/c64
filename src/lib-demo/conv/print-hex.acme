; Print hex numbers

            ;ACME 0.97
            !cpu 6510

            !src <lib/strings/m-print.acme>
            !src <lib/misc/upstart.acme>


main:
            ldy #0
-           lda numbers,y
            jsr print_hex
            +new_line
            iny
            cpy #HOW_MANY
            bne -

            rts


numbers:
            !byte $00, $01, $0a, $0f, $10, $aa, $ff
            HOW_MANY = * - numbers

            !src <lib/conv/print-hex.acme>
