; BASIC upstart demo.
;
; LOAD and RUN.

            ;ACME 0.97
            !cpu 6510

            !src <lib/mmap/zp-free.acme>
            !src <lib/misc/upstart.acme>

main:
            lda #<.hello
            sta R1
            lda #>.hello
            sta R2
